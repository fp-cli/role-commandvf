{
	"name": "wp-cli/wp-cli",
	"description": "A command line interface for WordPress",
	"keywords": [ "cli", "wordpress" ], 
	"homepage": "http://wp-cli.org",
	"license": "MIT",
	"bin": [
		"bin/wp"
	],
	"require": {
		"php": ">=5.3.2",
		"wp-cli/php-cli-tools": "0.9.3",
		"mustache/mustache": "~2.3"
	},
	"suggest": {
		"d11wtq/boris": "Enhanced `wp shell` functionality"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7",
		"behat/behat": "~2.4"
	},
	"autoload": {
		"psr-0": { "WP_CLI": "php" }
	}
}
